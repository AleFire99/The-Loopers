function [Pxs] = fft2data(data,Uvec,Ts )

%data=data(:,500*8:end) ;
%data = data(:,1:500*0.5);
Ts = 0.002;
downsample_factor   =   1;
Ts                  =   Ts*downsample_factor;

%% Use noise-corrupted values
theta           = data(1,:);               % Measured sideslip angle
alfa            = data(3,:);                % Measured yaw rate
Pxs = [];

Pxs(1,:)= applyfft(Uvec,theta);
Pxs(2,:)= applyfft(Uvec,alfa);


end

